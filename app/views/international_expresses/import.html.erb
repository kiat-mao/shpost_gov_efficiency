<br>
<font face="微软雅黑" color='#006030' size=6>待跟踪邮件信息导入</font>   
<%= form_tag({:action => 'import'}, :multipart => true, name: 'form1') do %>
<table>
    <tr style="line-height: 40px;height: 40px;"> 
      <td><%= label_tag(:country, "国家:") %></td>
      <td><%= select_tag(:country, options_for_select(CountryTimeLimit.all.map{|c| [c.country,c.id]}, :selected=> params[:country]), id: "country") %></td>
    </tr>
</table>
<br><br>
<font face="微软雅黑" color='#D9B300' size=3>上传文档模板: <%= file_field("file", "file",onclick: "$('#submit_button').removeAttr('disabled');") %></font>  
<br><br>

<%= submit_tag "确认导入",:id=>'submit_button', :class => 'btn btn-primary', onclick: "form1.submit();this.disabled='disabled';" %>
<% end -%>    