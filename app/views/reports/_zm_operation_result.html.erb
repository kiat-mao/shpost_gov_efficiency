<% if !@results.blank? %>
<table data-toggle="table" data-height="700">
  <tr class= 'nowrap'>
    <td colspan="5"><b>中免日上互联科技有限公司运营日报</b></td>
    <td><b>汇总</b></td>
    <td></td>
    <td colspan="3"><b>华东区域江浙沪皖次日达时效</b></td>
    <td colspan="2"></td>
	</tr>
	<tr class= 'nowrap'>
		<td><b>日期</b></td>
		<td><b>接收总单呈</b></td>
		<td><b>在途</b></td>
		<td><b>未妥投</b></td>
		<td><b>妥投</b></td>
		<td><b>妥投率</b></td>
		<td><b>接收总单量</b></td>
		<td><b>妥投</b></td>
		<td><b>次日达</b></td>
		<td><b>占比</b></td>
		<td><b>运输中</b></td>
		<td><b>配送中</b></td>
	</tr>
	<!-- 总计 -->
  <tr class= 'nowrap'>
  	<% 0.upto(11) do |i| %>
  		<td><%= @results[1][i] %></td>
  	<% end %>
  </tr>
  <!-- 明细 -->  
	<% @results[0].each do |k, v| %>
	<tr class= 'nowrap'>
		<td><%= k %></td>
		<% 0.upto(10) do |j| %>
			<td><%= v[j] %></td> 
		<% end %>
	</tr>
	<% end %>
</table>
<% end %>

