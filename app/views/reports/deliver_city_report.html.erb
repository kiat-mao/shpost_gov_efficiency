      <td><%= v[23].to_s(:rounded, precision: 2)+"%" %></td>
      <td><u><%= link_to v[24], get_expresses_path("/expresses", nil, "delivered", nil, nil, nil, nil, nil, "3.5", k[0].blank? ? "nil" : k[0], nil, nil), target: '_blank' %></u></td>
