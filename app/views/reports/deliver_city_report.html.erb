      <td><u><%= link_to v[12], get_expresses_path("/expresses", nil, "delivered", nil, nil, nil, nil, nil, "4", params[:receiver_province_no], k[0].blank? ? "nil" : k[0], nil), target: '_blank' %></u></td>
      <td id='waiting'><%= link_to v[13], get_expresses_path("/expresses", nil, "waiting", nil, nil, nil, nil, nil, nil, params[:receiver_province_no], k[0].blank? ? "nil" : k[0], nil), target: '_blank' %></td>
