        <th><b>退回数</b></th>
        <th><b>退回率</b></th>
      </tr>
    </thead>
    <tbody>
    <% @results.each do |k, v| %>
     <tr class= 'nowrap'>
      <% if !k[0].blank? && !(k[0].eql?"合计") %>
        <td><%= k[1].blank? ? k[0] : k[1] %></td>
