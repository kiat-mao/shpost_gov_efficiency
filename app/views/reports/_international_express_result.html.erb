<% if !@results.blank? %>
  <table data-toggle="table" 
  data-height="700">
    <thead>
      <tr class= 'nowrap'>
        <th rowspan="2"><b>序号</b></th>
        <th rowspan="2"><b>客户名称</b></th>
        <th rowspan="2"><b>寄达国</b></th>
        <th rowspan="2"><b>地区</b></th>
        <th rowspan="2"><b>业务量</b></th>
        <th rowspan="2"><b>重量（克）</b></th>
        <th rowspan="2"><b>退回妥投量</b></th>
        <th rowspan="2"><b>退回妥投占比</b></th>
        <th rowspan="2"><b>退回妥投量（安检未过）</b></th>
        <th colspan="3"><b>总包到达寄达地(T+24)</b></th>
        <th colspan="3"><b>总包到达寄达地(>T+24)</b></th>
        <th colspan="3"><b>离开境外处理中心(T+48)</b></th>
        <th colspan="3"><b>离开境外处理中心(>T+48)</b></th>
        <th colspan="3"><b>收寄局已封车(T)</b></th>
        <th colspan="3"><b>收寄局未封车(其他)</b></th>
        <th colspan="3"><b>互换局封车1(T+12)</b></th>
        <th colspan="3"><b>互换局封车2(T+36+18)</b></th>
        <th colspan="3"><b>互换局未及时封车</b></th>
        <th colspan="3"><b>互换局已封车</b></th>
        <th colspan="3"><b>互换局未封车</b></th>
        <th colspan="3"><b>航空启运信息(T+24)</b></th>
        <th colspan="3"><b>航空启运信息(>T+24)</b></th>
      </tr>
      <tr>
      	<% i=0
      		 while i<13 %>
      	<th><b>业务量</b></th>
      	<th><b>重量（克）</b></th>
      	<th><b>占比</b></th>
      	<% i +=1 
      	   end %>
  	  </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
<% end %>