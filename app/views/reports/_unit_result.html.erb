<% if !@results.blank? %>
  <table data-toggle="table" 
  data-height="700">
    <thead>
      <tr class= 'nowrap'>
        <th><b>单位</b></th>
        <th><b>网点</b></th>
        <th><b>总邮件数</b></th>
        <th><b>总妥投数</b></th>
        <th><b>本人收数</b></th>
        <th><b>他人收数</b></th>
        <th><b>单位/快递柜收数</b></th>
        <th><b>妥投率</b></th>
        <% 
          if !@is_monitor
            d = 0.0

            while d <= params[:delivered_days_show].to_f-1
        %>
              <th><b><%= Report::DELIVERED_DAYS_NAME[d]%>妥投率</b></th>
        <%
              d += 0.5
            end
        %>
          <th><b><%= Report::DELAY_DELIVERED_NAME[params[:delivered_days_show].to_f] %>日未妥投数</b></th>
        <%
          end
        %>       
        <th><b>未妥投总数</b></th>
        <th><b>在途中数</b></th>
        <th><b>投递端数</b></th>
        <th><b>未妥投率</b></th>
        <th><b>退回数</b></th>
        <th><b>退回率</b></th>
      </tr>
    </thead>
    <tbody>
    <% last_pid = @results.values[0] %>
    <% @results.each do |k, v| %>
      <% if !@is_monitor && !@is_court && (v[0] != last_pid) && !@parent_unit_results[last_pid].blank? %>
      <tr class= 'nowrap'>
        <!-- 单位 -->
        <td><%= @parent_unit_results[last_pid][0] %></td>
        <!-- 网点 -->
        <td></td>
        <!-- 总邮件数 -->
        <td><u><%= link_to @parent_unit_results[last_pid][1], get_expresses_path("/expresses", nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, last_pid), target: '_blank' %></u></td>
        <!-- 总妥投数 -->
        <td><u><%= link_to @parent_unit_results[last_pid][2], get_expresses_path("/expresses", nil, "delivered", nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, last_pid), target: '_blank' %></u></td>
        <!-- 本人收数 -->
        <td><u><%= link_to @parent_unit_results[last_pid][3], get_expresses_path("/expresses", nil, "delivered", nil, nil, nil, nil, nil, nil, nil, nil, "own", nil, nil, last_pid), target: '_blank' %></u></td>
        <!-- 他人收数 -->
        <td><u><%= link_to @parent_unit_results[last_pid][4], get_expresses_path("/expresses", nil, "delivered", nil, nil, nil, nil, nil, nil, nil, nil, "other", nil, nil, last_pid), target: '_blank' %></u></td>
        <!-- 单位/快递柜收数 -->
        <td><u><%= link_to @parent_unit_results[last_pid][5], get_expresses_path("/expresses", nil, "delivered", nil, nil, nil, nil, nil, nil, nil, nil, "unit", nil, nil, last_pid), target: '_blank' %></u></td>
        <!-- 妥投率 -->
        <td><%= @parent_unit_results[last_pid][6].to_s(:rounded, precision: 2)+"%" %></td>
        <!-- **日妥投率 -->
        <%  if !@is_monitor
              i = 0.0

              while i <= params[:delivered_days_show].to_f-1
        %>
                <td><%= (@parent_unit_results[last_pid][14][i]/@parent_unit_results[last_pid][1].to_f*100).to_s(:rounded, precision: 2)+"%" %></td>
        <%
                i += 0.5
              end
        %>
        <!-- 未及时妥投数 -->
        <td id='waiting'><%= link_to @parent_unit_results[last_pid][13], get_expresses_path("/expresses", nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, "true", last_pid), target: '_blank' %></td>
        <%
            end
        %>       
        <!-- 未妥投总数 -->
        <td id='waiting'><%= link_to @parent_unit_results[last_pid][7], get_expresses_path("/expresses", nil, "waiting", nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, last_pid), target: '_blank' %></td>
        <!-- 在途中数 -->
        <td id='waiting'><%= link_to @parent_unit_results[last_pid][11], get_expresses_path("/expresses", nil, "waiting", nil, nil, "in_transit", nil, nil, nil, nil, nil, nil, nil, nil, last_pid), target: '_blank' %></td>
        <!-- 投递端数 -->
        <td id='waiting'><%= link_to @parent_unit_results[last_pid][12], get_expresses_path("/expresses", nil, "waiting", nil, nil, "delivery_part", nil, nil, nil, nil, nil, nil, nil, nil, last_pid), target: '_blank' %></td>
        <!-- 未妥投率 -->
        <td><font color="red"><%= @parent_unit_results[last_pid][8].to_s(:rounded, precision: 2)+"%" %></font></td>
        <!-- 退回数 -->
        <td><u><%= link_to @parent_unit_results[last_pid][9], get_expresses_path("/expresses", nil, "returns", nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, last_pid), target: '_blank' %></u></td>
        <!-- 退回率 -->
        <td><%= @parent_unit_results[last_pid][10].to_s(:rounded, precision: 2)+"%" %></td>
      </tr>
      <% end %>
       <tr class= 'nowrap'>
        <!-- 单位 -->
        <td><%= (k.blank? || (k.eql?"合计")) ? "" : ((v[0] == last_pid) ? "" : v[14]) %></td>
        <!-- 网点 -->
        <td><%= k.blank? ? "其他" : ((k.eql?"合计") ? k : v[13]) %></td>
        <!-- 总邮件数 -->
        <td><u><%= link_to v[1], get_expresses_path("/expresses", nil, nil, (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil), target: '_blank' %></u></td>
        <!-- 总妥投数 -->
        <td><u><%= link_to v[2], get_expresses_path("/expresses", nil, "delivered", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil), target: '_blank' %></u></td>
        <!-- 本人收数 -->
        <td><u><%= link_to v[3], get_expresses_path("/expresses", nil, "delivered", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, "own", nil, nil, nil), target: '_blank' %></u></td>
        <!-- 他人收数 -->
        <td><u><%= link_to v[4], get_expresses_path("/expresses", nil, "delivered", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, "other", nil, nil, nil), target: '_blank' %></u></td>
        <!-- 单位/快递柜收数 -->
        <td><u><%= link_to v[5], get_expresses_path("/expresses", nil, "delivered", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, "unit", nil, nil, nil), target: '_blank' %></u></td>
        <!-- 妥投率 -->
        <td><%= v[6].to_s(:rounded, precision: 2)+"%" %></td>
        <!-- **日妥投率 -->
        <%  if !@is_monitor
              i = 0.0

              while i <= params[:delivered_days_show].to_f-1
        %>
                <td><%= v[15][i][1].to_s(:rounded, precision: 2)+"%" %></td>
        <%
                i += 0.5
              end
        %>
        <!-- 未及时妥投数 -->
        <td id='waiting'><%= link_to v[16], get_expresses_path("/expresses", nil, nil, (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, nil, nil, "true", nil), target: '_blank' %></td>
        <%
            end
        %>       
        <!-- 未妥投总数 -->
        <td id='waiting'><%= link_to v[7], get_expresses_path("/expresses", nil, "waiting", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil), target: '_blank' %></td>
        <!-- 在途中数 -->
        <td id='waiting'><%= link_to v[11], get_expresses_path("/expresses", nil, "waiting", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, "in_transit", nil, nil, nil, nil, nil, nil, nil, nil, nil), target: '_blank' %></td>
        <!-- 投递端数 -->
        <td id='waiting'><%= link_to v[12], get_expresses_path("/expresses", nil, "waiting", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, "delivery_part", nil, nil, nil, nil, nil, nil, nil, nil, nil), target: '_blank' %></td>
        <!-- 未妥投率 -->
        <td><font color="red"><%= v[8].to_s(:rounded, precision: 2)+"%" %></font></td>
        <!-- 退回数 -->
        <td><u><%= link_to v[9], get_expresses_path("/expresses", nil, "returns", (k.blank? ? "其他" : ((k.eql?"合计") ? k : k)), nil, nil, nil, nil, nil, nil, nil, nil, nil, nil, nil), target: '_blank' %></u></td>
        <!-- 退回率 -->
        <td><%= v[10].to_s(:rounded, precision: 2)+"%" %></td>
        <% last_pid = v[0] %>
      </tr>
    <% end %>
    </tbody>
  </table>
<% end %>

