<% if !@results.blank? %>
<table data-toggle="table" data-height="700">
  <thead>
    <tr class= 'nowrap'>
      <th><b>收件省份</b></th>
      <th><b>在途总量(件)</b></th>
      <th><b>时限内在途量(件)</b></th>
      <th><b>超时在途量(件)</b></th>
    </tr>
  </thead>
  <tbody>
  	<% @results[0].each do |k, v| %>
  	<tr class= 'nowrap'>
			<td><%= k[1] %></td>
			<td><u><%= link_to v[0], get_zm_expresses_path("/expresses/zm_province_index", nil, nil, nil, "waiting", "1100207488562", k[0], nil, nil), target: '_blank' %></u></td>
			<td><u><%= link_to v[1], get_zm_expresses_path("/expresses/zm_province_index", nil, nil, nil, "waiting", "1100207488562", k[0], nil, "false"), target: '_blank' %></u></td>
			<td><u><%= link_to v[2], get_zm_expresses_path("/expresses/zm_province_index", nil, nil, nil, "waiting", "1100207488562", k[0], nil, "true"), target: '_blank' %></u></td>
		</tr>
    <% end %>
    <!-- 总计 -->
    <tr class= 'nowrap'>
    	<td><%= @results[1][0] %></td>
    	<td><u><%= link_to @results[1][1], get_zm_expresses_path("/expresses/zm_province_index", nil,nil, nil, "waiting", "1100207488562", nil, nil, nil), target: '_blank' %></u></td>
    	<td><u><%= link_to @results[1][2], get_zm_expresses_path("/expresses/zm_province_index", nil,nil, nil, "waiting", "1100207488562", nil, nil, "false"), target: '_blank' %></u></td>
    	<td><u><%= link_to @results[1][3], get_zm_expresses_path("/expresses/zm_province_index", nil,nil, nil, "waiting", "1100207488562", nil, nil, "true"), target: '_blank' %></u></td>
    </tr>
  </tbody>
</table>
<% end %>
