
<br>
<font face="微软雅黑" color='#006030' size=6>投递情况监控报表-省市维度</font>   
<%= form_tag(deliver_province_report_reports_path, name: 'form', method: 'post') do %>
<%= render :partial => 'form' %>
<table>
    <tr>
      <td>
        <div id="market_monitor_search">
          <%= submit_tag "查询", :class => 'btn btn-primary', method: :post %></td>
        </div>
      <td>
        <div id="export">
          <%= link_to "报表导出", deliver_prov_city_report_export_reports_path(industry: params[:industry], btype: params[:btype], business: params[:business], posting_date_start: params[:posting_date_start], posting_date_end: params[:posting_date_end], search_time: params[:search_time], year: params[:year], month: params[:month], destination: params[:destination], product: params[:product], distributive_center_no: params[:distributive_center_no], posting_hour_start: params[:posting_hour_start], posting_hour_end: params[:posting_hour_end]), :class => 'btn btn-primary', method: :post%>
        </div>
      </td>
    </tr>
  </table>
<% end -%>    
  
<br>
<% if !@results.blank? %>
  <table data-toggle="table" 
  data-height="700">
    <thead>
      <tr class= 'nowrap'>
        <th><b>收寄省</b></th>
        <th><b>收寄数</b></th>
        <th><b>总妥投数</b></th>
        <th><b>妥投率</b></th>
        <th><b>平均妥投天数</b></th>
        <th><b>次日妥投率</b></th>
        <th><b>次日妥投总数</b></th>
        <th><b>三日妥投率</b></th>
        <th><b>三日妥投总数</b></th>
        <th><b>五日妥投率</b></th>
        <th><b>五日妥投总数</b></th>
        <th><b>未妥投总数</b></th>
        <th><b>未妥投率</b></th>
        <th><b>退回数</b></th>
        <th><b>退回率</b></th>
      </tr>
    </thead>
    <tbody>
    <% @results.each do |k, v| %>
     <tr class= 'nowrap'>
      <% if !k[0].blank? %>
        <td><u><%= link_to k[1].blank? ? "其他" : k[1], deliver_city_report_reports_path(industry: params[:industry], btype: params[:btype], business: params[:business], posting_date_start: params[:posting_date_start], posting_date_end: params[:posting_date_end], destination: params[:destination], search_time: params[:search_time], year: params[:year], month: params[:month], product: params[:product], distributive_center_no: params[:distributive_center_no], posting_hour_start: params[:posting_hour_start], posting_hour_end: params[:posting_hour_end], receiver_province_no: k[0]), target: '_blank' %></u></td>
      <% else %>
        <td><%= k[1].blank? ? "其他" : k[1] %></td>
      <% end %>
      <td><u><%= link_to v[0], get_expresses_path(nil, nil, nil, nil, nil, nil, nil,nil, k[0], nil), target: '_blank' %></u></td>
      <td><u><%= link_to v[1], get_expresses_path(nil, "delivered", nil, nil, nil, nil, nil, nil, k[0], nil), target: '_blank' %></u></td>
      <td><%= v[2].to_s(:rounded, precision: 2)+"%" %></td>
      <td><%= v[3].to_s(:rounded, precision: 2) %></td>
      <td><%= v[4].to_s(:rounded, precision: 2)+"%" %></td>
      <td><u><%= link_to v[5], get_expresses_path(nil, "delivered", nil, nil, nil, nil, nil, "2", k[0], nil), target: '_blank' %></u></td>
      <td><%= v[6].to_s(:rounded, precision: 2)+"%" %></td>
      <td><u><%= link_to v[7], get_expresses_path(nil, "delivered", nil, nil, nil, nil, nil, "3", k[0], nil), target: '_blank' %></u></td>
      <td><%= v[8].to_s(:rounded, precision: 2)+"%" %></td>
      <td><u><%= link_to v[9], get_expresses_path(nil, "delivered", nil, nil, nil, nil, nil, "5", k[0], nil), target: '_blank' %></u></td>
      <td id='waiting'><%= link_to v[10], get_expresses_path(nil, "waiting", nil, nil, nil, nil, nil, nil, k[0], nil), target: '_blank' %></td>
      <td><font color="red"><%= v[11].to_s(:rounded, precision: 2)+"%" %></font></td>
      <td><u><%= link_to v[12], get_expresses_path(nil, "returns", nil, nil, nil, nil, nil, nil, k[0], nil), target: '_blank' %></u></td>
      <td><%= v[13].to_s(:rounded, precision: 2)+"%" %></td>
    </tr>
    <% end %>
    </tbody>
  </table>
<% end %>

